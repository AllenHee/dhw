@{
    ViewBag.Title = "订单完成 - 梦高速";
    var data = Model as System.Data.DataRow;
}
<!--主内容区 开始-->
<ul class="pay_progress clearfix">
    <li class="pay_progress_items">1.订单信息填写 <i class="triangle white_trian"></i><i class="triangle grey_trian">
    </i></li>
    <li class="pay_progress_items">2.订单信息确认 <i class="triangle white_trian"></i><i class="triangle grey_trian">
    </i></li>
    <li class="pay_progress_items">3.支付 <i class="triangle white_trian"></i><i class="triangle grey_trian">
    </i></li>
    <li class="pay_progress_items complete">4.完成 </li>
</ul>
<div class="content">
    <div class="pay_l">
        <img src="@{@BaseConfigs.GetStaticPath}zhongchou/images/images/wancheng.png" alt="">
        <p class="text">@data["stateName"]！请耐心等待众筹项目结束！</p>
        <p class="number">
            订单号 ：@data["number"]</p>
        <p class="online">
            在线支付 ：@data["cost"]</p>
        <a class="details" href="#">查看订单详情</a>
        <form>
        <input class="going" type="button" value="继续逛逛">
        </form>
    </div>
    <div class="pay_r">
        <img src="@{@BaseConfigs.GetStaticPath}zhongchou/images/erweima.jpg" alt="">
        <p>
            安装手机客户端实时跟踪物流信息</p>
    </div>
    <div class="pay_b">
        <p class="important">
            重要提示 ：</p>
        <p>
            梦高速平台及销售商不会以<span>订单异常、系统升级</span>为由要求您点击任何连接进行退款。</p>
        <p>
            详情请关注<a class="notice" href="#">《谨防诈骗公告》</a></p>
    </div>
</div>
<!--主内容区 结束-->
@section scripts{
}
