@section scripts{
    <script src="@BaseConfigs.GetStaticPath/scripts/template.js"></script>
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/shangbiao.js"></script>
    <script type="text/html" id="firTem">
  {{each firPic as fir}}
    <li class="index_trademark_show_item">
     <a href="/trademark/{{fir.id}}"> <img src="{{imgurl}}{{fir.img}}" alt="">
      <div class="index_trademark_mask">
        <span class="index_trademark_mask_pri"><span>￥</span>{{fir.price}}</span>
        <span class="index_trademark_mask_ttl">{{fir.name}}</span>
      </div></a>
    </li>
  {{/each}}
    </script>
    <script type="text/html" id="secTem">
  {{each secPic as sec}}
    <li class="index_trademark_show_item">
      <img src="{{imgurl}}{{sec.img}}" alt="">
      <div class="index_trademark_mask">
        <span class="index_trademark_mask_pri"><span>￥</span>{{sec.price}}</span>
        <span class="index_trademark_mask_ttl">{{sec.name}}</span>
      </div>
    </li>
  {{/each}}
    </script>
    <script type="text/html" id="thiTem">
  {{each thiPic as thi}}
    <li class="index_trademark_show_item">
      <img src="{{imgurl}}{{thi.img}}" alt="">
      <div class="index_trademark_mask">
        <span class="index_trademark_mask_pri"><span>￥</span>{{thi.price}}</span>
        <span class="index_trademark_mask_ttl">{{thi.name}}</span>
      </div>
    </li>
  {{/each}}
    </script>
    <script type="text/html" id="fouTem">
  {{each fouHtml as fou}}
    <li class="index_trademark_show_item">
      <img src="{{imgurl}}{{fou.img}}" alt="">
      <div class="index_trademark_mask">
        <span class="index_trademark_mask_pri"><span>￥</span>{{fou.price}}</span>
        <span class="index_trademark_mask_ttl">{{fou.name}}</span>
      </div>
    </li>
  {{/each}}
    </script>
    <script type="text/html" id="typeName">
  {{each types as type}}
  <span>{{type.name}}</span>
  {{/each}}
    </script>
