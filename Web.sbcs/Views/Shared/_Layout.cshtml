@{
    Layout = "~/Views/Shared/_BaseLayout.cshtml";
}
@section head{
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}sbcs/styles/sbcs.css">
    @RenderSection("head", required: false)
}
@section scripts{
    @RenderSection("scripts", required: false)
}
@{string ap = Request.Url.AbsolutePath.ToLower();}
<!--头部 开始-->
<div class="sbcs">
    <div class="sbcs_head">
        <div class="index_logo">
            <img src="@{@BaseConfigs.GetUrlCdn}sbcs/images/logo.png" alt="">
        </div>
        <ul class="sbcs_nav clearfix">
            <li class="sbcs_nav_item sbcs_nav_item-current">首页</li>
            <li class="sbcs_nav_item">商标精选</li>
            <li class="sbcs_nav_item">商标分类</li>
            <li class="sbcs_nav_item">快速发布</li>
        </ul>
    </div>
</div>
<!--头部 结束-->
@RenderBody()
