@{
    ViewBag.Title = "梦高速 - 商标超市";
}
<!--主内容 开始-->
<div class="sbcs_content">
    @Helper1.display_query_cagetory(ViewBag.Category)
    <div class="sbcs_filterbox">
      @Helper1.display_query_price()
    </div>
    <ul class="sbcs_jingpin">
      
    </ul>
    <div class="pagination"></div>

  </div>

 
<!--主内容 结束-->
@section scripts{
      <script type="text/html" id="jpTem">
  {{each data as jp}}
  <li class="sbcs_jingpin_list clearfix">
    <div class="sbcs_jingpin_listl">
      <img class="sbcs_jingpin_listl_img" src="{{domain}}{{jp.img}}" alt="图标">
      <h4 class="sbcs_jingpin_listl_til">{{jp.name}}</h4>
      <p class="sbcs_jingpin_listl_leibie">
        {{each jp.type as type}}
        <span>{{type}}</span>
        {{/each}}
      </p>
      <span class="sbcs_jingpin_listl_price">￥{{jp.price}}</span>
      <span class="sbcs_jingpin_listl_detail">了解详情</span>
    </div>
    <div class="sbcs_jingpin_listr">
      <img src="{{domain}}{{jp.boutiqueLogo}}" alt="主图">
    </div>
  </li>
  {{/each}}
  </script>




<script src="@{@BaseConfigs.GetUrlCdn}scripts/template.js"></script>
  <script src="@{@BaseConfigs.GetUrlCdn}scripts/jquery.paging.min.js"></script>
  <script src="@{@BaseConfigs.GetUrlCdn}scripts/jquery.loaddata.js"></script>
  <script src="@{@BaseConfigs.GetUrlCdn}scripts/sb_jingpin.js"></script>
    
}
