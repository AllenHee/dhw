@{ViewBag.Title = "梦高速 - 个人中心 - 众筹";}
@section head{
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}zhongchou/styles/zhongchou.css">
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}usercenter/styles/zcForm.css">
    <script type="text/javascript">        if (dhw) dhw.gettplurl = function (name) { return function () { return '/modulezc/' + name + '?_=' + (+new Date()) } }</script>
}
<!--主内容 开始-->
<div class="content clearfix">
    <!--左栏 开始-->
    <div class="cl">
        <dl class="cygn">
            <dt>常用功能</dt>
            @if (ViewBag.LoginAsCompany)
            {
                <dd>
                    <a href="#/">发布项目</a></dd>
                <dd>
                    <a href="#/list">已发布项目查询</a></dd>
            }
            <dd>
                <a href="#/gzlist">关注列表</a></dd>
            <dd>
                <a href="#/tzlist">支持列表</a></dd>
        </dl>
    </div>
    <!--左栏 结束-->
    <!--中栏 开始-->
    <div class="cm">
        <div class="clearfix" ng-app="zcApp" ui-view>
        </div>
    </div>
    <!--中栏 结束-->
    <!--右栏 开始-->
    <div class="cr">
    </div>
    <!--右栏 结束-->
</div>
<!--主内容 结束-->
@section scripts{
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/data.js"></script>
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/plupload.full.min.js"></script>
    @Helper1.Displayjs(BaseConfigs.GetUrlCdn)
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/uc.zcFormCtrl.js"></script>
}
