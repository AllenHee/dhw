@{ViewBag.Title = "梦高速 - 个人中心 - 众创空间";}
@section head{
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}usercenter/styles/zpform.css">
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}usercenter/styles/login.css">
    <script type="text/javascript">        if (dhw) dhw.gettplurl = function (name) { return function () { return '/modulezckj/' + name + '?_=' + (+new Date()) } }</script>
}
<!--主内容 开始-->
<div class="content clearfix">
    <!--左栏 开始-->
    <div class="cl">
        <dl class="cygn">
            <dt>常用功能</dt>
            @if (ViewBag.LoginAsCompany)
            {
                <dd>
                    <a href="#/apply">申请入驻</a></dd>
                <dd>
                    <a href="#/list">入驻列表</a></dd>
            }
        </dl>
    </div>
    <!--左栏 结束-->
    <!--中栏 开始-->
    <div class="cm">
        <div class="clearfix" ng-app="AppZckj" ui-view>
        </div>
    </div>
    <!--中栏 结束-->
    <!--右栏 开始-->
    <div class="cr">
    </div>
    <!--右栏 结束-->
</div>
<!--主内容 结束-->
@section scripts{
    @Helper1.Displayjs(BaseConfigs.GetUrlCdn)
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/uc.ZckjCtrl.js"></script>
    @*<script src="/cdn/uc.ZckjCtrl.js"></script>*@
}
