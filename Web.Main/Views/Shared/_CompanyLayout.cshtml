@{ string ap = Request.Url.AbsolutePath.ToLower();
   var CompanyTopInfo = DHW.Main.Controllers.CompanyHomeController.CompanyTopInfo(ViewBag.ddid, ViewBag.DUser.ID); 
}
<!DOCTYPE html>
<html lang="zh">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <title>@ViewBag.Title</title>
    <meta name="description" content="@ViewBag.Description">
    <meta name="keywords" content="@ViewBag.Keywords">
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}fonts/iconfont.css">
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}styles/general.css">
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}usercenter/styles/cmpy.css">
    <style>
        .sc {
        color: @CompanyTopInfo.theme;
        border-color:@CompanyTopInfo.theme;
        }
        .bds_more {
        color: @CompanyTopInfo.theme !important;
        border-color:@CompanyTopInfo.theme;
        }
        .sc, .share {
        border-color:@CompanyTopInfo.theme;
        }
        .nav_wrap {
        background-color:@CompanyTopInfo.theme;
        }
        .header_r input {
        border-color:@CompanyTopInfo.theme;
        }
        .header_r a{
        background-color:@CompanyTopInfo.theme;
        }
        .header_r a i {
        background-color:@CompanyTopInfo.theme;
        }

    </style>
    <script type="text/javascript">
        var dhw = {
            imgurl: '@BaseConfigs.GetImgPath', imguploadurl: '@BaseConfigs.GetImgUploadPath', imgcuturl: '@BaseConfigs.GetImgCutPath',
            mainurl: '@BaseConfigs.GetUrlMain', staticurl: '@BaseConfigs.GetStaticPath',
            urlmain: '@BaseConfigs.GetUrlMain', urlcdn: '@BaseConfigs.GetUrlCdn', urlzc: '@BaseConfigs.GetUrlZc', urlkj: '@BaseConfigs.GetUrlKj', urlhr: '@BaseConfigs.GetUrlHr', urlzckj: '@BaseConfigs.GetUrlZckj',
            urlzb: '@BaseConfigs.GetUrlZb', urldiy: '@BaseConfigs.GetUrlDiy', urlsrdz: '@BaseConfigs.GetUrlSrdz', urlcqbh: '@BaseConfigs.GetUrlCqbh', urlzxsj: '@BaseConfigs.GetUrlZxsj', urlgsss: '@BaseConfigs.GetUrlGsss',
            urlfj: '@BaseConfigs.GetUrlFj', urlimg: '@BaseConfigs.GetUrlImg', urlfjupload: '@BaseConfigs.GetUrlFjUpload'
        };
    </script>
    <script type="text/javascript">
        var readCookie = function (a) { var b = a + "="; var c = document.cookie.split(";"); for (var d = 0; d < c.length; d++) { var e = c[d]; for (; " " == e.charAt(0); ) e = e.substring(1, e.length); if (0 == e.indexOf(b)) return e.substring(b.length, e.length) } return null }
        var login = function () { return location.href = dhw.mainurl + "login?redirectURL=" + escape(location.href).replace(/\//g, "%2F"), !1 }; var regist = function () { return location.href = dhw.mainurl + "reg?redirectURL=" + escape(location.href).replace(/\//g, "%2F"), !1 };</script>
    @RenderSection("head", required: false)
</head>
<body>
    <!--通用顶栏 开始-->
    <div class="sitenav clearfix">
        <div class="sitenav-wrap">
            <ul class="sitenav-l">
                <li><a href="@BaseConfigs.GetUrlMain"><i class="iconfont">&#xe605;</i>梦高速首页</a></li>
                <li><a href="#"><i class="iconfont">&#xe609;</i>手机梦高速</a></li>
            </ul>
            <ul class="sitenav-r">
                <li class="log">您好，请<a href="javascript:login();">登陆</a><a href="javascript:regist();">
                    免费注册</a> </li>
                <li><a href="@BaseConfigs.GetUrlBbs" target="_blank">帮助中心</a></li>
                <li class="nav-drop"><a class="sn-dropper" href="#">网站导航 <i class="iconfont">&#xe60f;</i></a>
                    <ul class="sn-dropdown menu-dropdown">
                        <li><a target="_blank" href="@BaseConfigs.GetUrlKj">会计服务</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlHr">人才招聘</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlDiy">创意设计</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlZckj">众创空间</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlZc">众筹</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlZb">众包</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlCqbh">产权保护</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlSrdz">私人定制</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlCygc">创意工厂</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlZxsj">转型升级</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlGsss">公司上市</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlBbs">社区论坛</a></li>
                    </ul>
                </li>
                <li class="icon-drop"><i class="sn-dropper iconfont">&#xe60c;</i>
                    <div class="sn-dropdown">
                        <div>
                            <div>
                                <img src="@{@BaseConfigs.GetUrlCdn}images/weixin_temp.png" alt="微信二维码"></div>
                            微信</div>
                    </div>
                </li>
                <li class="icon-drop"><i class="sn-dropper iconfont">&#xe60a;</i>
                    <div class="sn-dropdown">
                        <div>
                            <img src="@{@BaseConfigs.GetUrlCdn}images/erweima.jpg" alt="微博"></div>
                        <div>
                            微博</div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!--通用顶栏 结束-->
    <div class="header">
        <div class="header_t clearfix">
            <div class="header_l">
                <a class="header_logo" href="">
                    <img src="@{@BaseConfigs.GetImgPath}@CompanyTopInfo.logo" alt=""></a>
                <div class="header_ttl">
                    <h2>
                        @CompanyTopInfo.compay
                        <i class="@if (CompanyTopInfo.auth == "True")
                                  {<text>trueAuth</text>}
                                  else
                                  {<text>falseAuth</text>}" ></i>
                    </h2>
                    @if (ViewBag.DUser.HasLogin)
                    {
                        if (CompanyTopInfo.sc != "1")
                        {
                        <a class="sc" href="javascript:void(0)" onclick="collect()">收藏</a> 
                        }
                        else
                        {
                        <a class="sc" href="javascript:void(0)" onclick="collect()">已收藏</a> 
                        }
                    }
                    else
                    {
                        <a class="sc" href="@{@BaseConfigs.GetUrlMain}login?redirectURL=@{@(Server.UrlEncode(Request.Url.ToString()))}">
                            收藏</a> 
                    }
                    <span class="share bdsharebuttonbox"><a class="hd_cont_l_share bds_more" href="#"
                        data-cmd="more">分享</a></span> <span class="xy">信用：<i></i><i></i><i></i><i></i><i></i>
                            <a class="more" href=""><span></span></a></span>
                </div>
            </div>
            <div class="header_r">
                <input class="emColorBorder" type="text">
                <a class="emColorBG" href="#"><i></i></a>
            </div>
        </div>
        <!--导航-->
        <div class="nav_wrap emColorBG">
            <ul class="nav clearfix">
                <li @if (ap.StartsWith("/company/index"))
                    {<text>class="current"</text>}><span></span><a href="/Company/Index/@ViewBag.ddid">首页</a></li>
                <li @if (ap.StartsWith("/company/caseshow"))
                    {<text>class="current"</text>}><span></span><a href="/Company/CaseShow/@ViewBag.ddid">
                        案例展示</a></li>
                <li @if (ap.StartsWith("/company/fwzx"))
                    {<text>class="current"</text>}><span></span><a href="/Company/Fwzx/@ViewBag.ddid">服务中心</a></li>
                <li @if (ap.StartsWith("/company/dongtai"))
                    {<text>class="current"</text>}><span></span><a href="/Company/DongTai/@ViewBag.ddid">
                        公司动态</a></li>
                <li @if (ap.StartsWith("/company/zhaopin"))
                    {<text>class="current"</text>}><span></span><a href="/Company/Zhaopin/@ViewBag.ddid">
                        公司招聘</a></li>
                @* <li @if (ap.StartsWith("/company/contactus"))
                {<text>class="current"</text>}><span></span><a href="/Company/Contactus/@ViewBag.ddid">
                    联系我们</a></li>*@
                <li @if (ap.StartsWith("/company/aboutus"))
                    {<text>class="current"</text>}><span></span><a href="/Company/Aboutus/@ViewBag.ddid">
                        关于我们</a></li>
            </ul>
        </div>
    </div>
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/uc.qiyeCtrl.js"></script>
    <script>
        with (document) 0[(getElementsByTagName('head')[0] || body).appendChild(createElement('script')).src = 'http://bdimg.share.baidu.com/static/api/js/share.js?v=89860593.js?cdnversion=' + ~(-new Date() / 36e5)]; 
    </script>
    @RenderBody()
    <!--通用底栏 开始-->
    <div class="gsFoot">
        <div class="gsFoot_content">
            <span class="gsFoot_text">©2015-2016 dreamhiway.com, All Rights Reserved 闽ICP备15011711号</span>
        </div>
    </div>
    <script src="@{@BaseConfigs.GetUrlCdn}static/lib/jquery.min.js"></script>
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/general.js"></script>
    <script>
        var ddid = @ViewBag.ddid;
    </script>
    <script type="text/javascript">
        $(function () {
            $.getJSON(dhw.urlmain + 'loginservice/Login?callback=?', function (r) {
                if (r.success && r.result.IsAuthenticated) {
                    dhw = dhw || {};
                    dhw.isLogined = true;
                    $('li.log').html('您好，<a href="' + dhw.urlmain + '' + "apphome" + '" target="_blank">' + r.result.Name + '</a><a href="' + dhw.urlmain + 'login?redirectURL=' + escape(location.href).replace(/\//g, "%2F") + '&type=logout">退出</a>');
                }
                else {
                    dhw.isLogined = false;
                    $('li.log').html('您好，请<a href="javascript:login();">登陆</a><a href="javascript:regist();">免费注册</a>');
                }
            });
        })
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
