        $(function () {
            var accountType = readCookie("accountType");
            if (accountType != null) {
                dhw = dhw || {};
                dhw.isLogined = true;
                var username = readCookie("name");
                var nickname = readCookie("nickname");
                var name;
                if (nickname == null) {
                    name = username;
                }
                else {
                    name = nickname + "(" + username + ")";
                }
                $('#bb01').html('您好，<a class="nav_right_list_item_login" href="' + dhw.urlmain + '' + "apphome" + '" target="_blank">' + name + '</a><a href="' + dhw.urlmain + 'login?redirectURL=' + escape(location.href).replace(/\//g, "%2F") + '&type=logout">退出</a>');
