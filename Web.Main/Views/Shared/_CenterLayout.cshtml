<!DOCTYPE html>
<html lang="zh">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <title>@ViewBag.Title</title>
    <meta name="description" content="@ViewBag.Description">
    <meta name="keywords" content="@ViewBag.Keywords">
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}fonts/iconfont.css">
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}styles/general.css">
    <link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}usercenter/styles/footer.css">
    <script type="text/javascript">
        var dhw = {
            imgurl: '@BaseConfigs.GetImgPath', imguploadurl: '@BaseConfigs.GetImgUploadPath', imgcuturl: '@BaseConfigs.GetImgCutPath',
            mainurl: '@BaseConfigs.GetUrlMain', staticurl: '@BaseConfigs.GetStaticPath',
            urlmain: '@BaseConfigs.GetUrlMain', urlcdn: '@BaseConfigs.GetUrlCdn', urlzc: '@BaseConfigs.GetUrlZc', urlkj: '@BaseConfigs.GetUrlKj', urlhr: '@BaseConfigs.GetUrlHr', urlzckj: '@BaseConfigs.GetUrlZckj',
            urlzb: '@BaseConfigs.GetUrlZb', urldiy: '@BaseConfigs.GetUrlDiy', urlsrdz: '@BaseConfigs.GetUrlSrdz', urlcqbh: '@BaseConfigs.GetUrlCqbh', urlzxsj: '@BaseConfigs.GetUrlZxsj', urlgsss: '@BaseConfigs.GetUrlGsss'
        };
    </script>
    <script type="text/javascript">
        var readCookie = function (a) { var b = a + "="; var c = document.cookie.split(";"); for (var d = 0; d < c.length; d++) { var e = c[d]; for (; " " == e.charAt(0); ) e = e.substring(1, e.length); if (0 == e.indexOf(b)) return e.substring(b.length, e.length) } return null }
        var login = function () { return location.href = dhw.mainurl + "login?redirectURL=" + escape(location.href).replace(/\//g, "%2F"), !1 }; var regist = function () { return location.href = dhw.mainurl + "reg?redirectURL=" + escape(location.href).replace(/\//g, "%2F"), !1 };</script>
    @RenderSection("head", required: false)
</head>
<body>
    <!--通用顶栏 开始-->
    <div class="sitenav clearfix">
        <div class="sitenav-wrap">
            <ul class="sitenav-l">
                <li><a href="@BaseConfigs.GetUrlMain"><i class="iconfont">&#xe605;</i>梦高速首页</a></li>
                <li><a href="#"><i class="iconfont">&#xe609;</i>手机梦高速</a></li>
            </ul>
            <ul class="sitenav-r">
                <li class="log">您好，请<a href="javascript:login();">登陆</a><a href="javascript:regist();">
                    免费注册</a> </li>
                <li><a href="http://bbs.dreamhiway.com" target="_blank">帮助中心</a></li>
                <li class="nav-drop"><a class="sn-dropper" href="#">网站导航 <i class="iconfont">&#xe60f;</i></a>
                    <ul class="sn-dropdown menu-dropdown">
                        <li><a target="_blank" href="@BaseConfigs.GetUrlKj">会计服务</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlHr">人才招聘</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlDiy">创意设计</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlZckj">众创空间</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlZc">众筹</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlZb">众包</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlCqbh">产权保护</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlSrdz">私人定制</a></li>
                        <li><a target="_blank" href="//cygc.dreamhiway.com/">创意工厂</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlZxsj">转型升级</a></li>
                        <li><a target="_blank" href="@BaseConfigs.GetUrlGsss">公司上市</a></li>
                        <li><a target="_blank" href="//bbs.dreamhiway.com/">社区论坛</a></li>
                    </ul>
                </li>
                <li class="icon-drop"><i class="sn-dropper iconfont">&#xe60c;</i>
                    <div class="sn-dropdown">
                        <div>
                            <div>
                                <img src="@{@BaseConfigs.GetUrlCdn}images/weixin_temp.png" alt="微信二维码"></div>
                            微信</div>
                    </div>
                </li>
                <li class="icon-drop"><i class="sn-dropper iconfont">&#xe60a;</i>
                    <div class="sn-dropdown">
                        <div>
                            <img src="@{@BaseConfigs.GetUrlCdn}images/erweima.jpg" alt="微博"></div>
                        <div>
                            微博</div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!--通用顶栏 结束-->
    @RenderBody()
    <!--通用底栏 开始-->
    <div class="footer">
        <div class="footer_content">
            <span class="">© 2015-2016 dreamhiway.com, All Rights Reserved 闽ICP备15011711号</span>
            <ul>
                <li><a href="http://bbs.dreamhiway.com" class="footer_a">免责声明</a> </li>
                <li><a href="http://bbs.dreamhiway.com" class="footer_a">帮助中心</a> </li>
                <li><a href="http://bbs.dreamhiway.com" class="footer_a">项目规范</a> </li>
                <li><a href="http://bbs.dreamhiway.com" class="footer_a">服务协议</a> </li>
            </ul>
        </div>
    </div>
    <script src="@{@BaseConfigs.GetUrlCdn}static/lib/jquery.min.js"></script>
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/general.js"></script>
    <script type="text/javascript">
        $(function () {
            $.getJSON(dhw.urlmain + 'loginservice/Login?callback=?', function (r) {
                if (r.success && r.result.IsAuthenticated) {
                    dhw = dhw || {};
                    dhw.isLogined = true;
                    var app;
                    var nickname = r.result.nickname;
                    var name;
                    if (r.result.type == 1) { app = "AppHome" } else { app = "AppHome_company" }
                    if (nickname == "") {
                        name = r.result.Name;
                    }
                    else {
                        name = nickname + "(" + r.result.Name + ")";
                    }
                    $('li.log').html('您好，<a href="' + dhw.urlmain + '' + app + '" target="_blank">' + name + '</a><a href="' + dhw.urlmain + 'login?redirectURL=' + escape(location.href).replace(/\//g, "%2F") + '&type=logout">退出</a>');
                }
                else {
                    dhw.isLogined = false;
                    $('li.log').html('您好，请<a href="javascript:login();">登陆</a><a href="javascript:regist();">免费注册</a>');
                }
            });
        })
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
