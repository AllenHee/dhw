@{
    Layout = Layout = "~/Views/Shared/_CompanyLayout.cshtml";
}
@{ string ap = Request.Url.AbsolutePath.ToLower();
   var CompanyTopInfo = DHW.Main.Controllers.CompanyHomeController.CompanyTopInfo(ViewBag.ddid, ViewBag.DUser.ID); 
}
@{
    var compay = CompanyTopInfo.compay;
    var trade = CompanyTopInfo.trade;
    ViewBag.Title = "服务中心" + compay;
    ViewBag.Keywords = compay + ",服务中心," + trade;
}
<link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}fonts/iconfont.css">
<link rel="stylesheet" href="@{@BaseConfigs.GetUrlCdn}styles/general.css">
<div class="banner">
    <img src="@{@BaseConfigs.GetUrlCdn}usercenter/images/qyz1.png" alt="">
</div>
<!--内容 开始-->
<div class="gsServe_wrap">
    <div class="gsServe">
        <div class="gsServe_top">
            <h3>
                出售服务</h3>
            <ul class="gsServe_choose clearfix">
                <li class="gsServe_choose_item"><a class="current" href="#">所有服务</a></li>
                <li class="gsServe_choose_item" data-type="1"><a href="javascript:void(0)">设计</a></li>
                <li class="gsServe_choose_item" data-type="2"><a href="javascript:void(0)">开发</a></li>
                <li class="gsServe_choose_item" data-type="3"><a href="javascript:void(0)">文案</a></li>
                <li class="gsServe_choose_item" data-type="4"><a href="javascript:void(0)">装修</a></li>
                <li class="gsServe_choose_item" data-type="5"><a href="javascript:void(0)">营销</a></li>
                <li class="gsServe_choose_item" data-type="6"><a href="javascript:void(0)">商务</a></li>
                <li class="gsServe_choose_item" data-type="7"><a href="javascript:void(0)">生活</a></li>
            </ul>
        </div>
        <div class="gsServe_main_wrap">
            <ul class="gsServe_main clearfix">
            </ul>
            <div class="pagination pagination-jl">
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script type="text/javascript">
    var ddid = @ViewBag.ddid;
    </script>
    <script type="text/html" id="server">
    {{each data as server}}
    <li>
      <img src='{{domain}}{{server.img}}_282x284.jpg' alt="">
      <div class="gsServe_main_txt">
        <span class="gsServe_main_txt_money">{{server.money}}</span></br>
        <a href="{{server.url}}" target="_blank"><span>{{server.name}}</span></a>
      </div>
    </li>
    {{/each}}
    </script>
    <script src="@{@BaseConfigs.GetUrlCdn}lib/angular/angular.min.js"></script>
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/jquery.paging.min.js"></script>
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/template.js"></script>
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/jquery.loaddata.js"></script>
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/serverCtrl.js"></script>
}
