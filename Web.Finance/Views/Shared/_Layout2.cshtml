@{ Layout = "~/Views/Shared/_Layout.cshtml";}
<!--头部 开始-->
<div class="header">
    <!--头部主内容 开始-->
    <div class="header-main clearfix">
        <div class="header-m-l">
            <a href="/">
                <h1>
                    <img src="@Html.Raw(BaseConfigs.GetUrlCdn)kuaiji/images/kuaiji_logo.jpg" alt="梦高速 - 会计"></h1>
            </a>
        </div>
        <div class="header-m-r">
            <div class="header-search clearfix">
                <form action="#"><span></span>
                <input type="text" placeholder="请输入关键词">
                <button type="submit">
                    搜索</button>
                </form>
            </div>
        </div>
    </div>
    <!--头部主内容 结束-->
    <!--头部轮播 开始-->
    <div class="slide-bg">
        <div class="slide">
            <ul class="slide-wrap">
                <a class="slide_bg1" href="#">
                    <li>
                        <img src="@Html.Raw(BaseConfigs.GetUrlCdn)kuaiji/images/slide_1.jpg" alt=""></li></a>
                <a class="slide_bg2" href="#">
                    <li>
                        <img src="@Html.Raw(BaseConfigs.GetUrlCdn)kuaiji/images/slide_2.jpg" alt=""></li></a>
                <a class="slide_bg3" href="#">
                    <li>
                        <img src="@Html.Raw(BaseConfigs.GetUrlCdn)kuaiji/images/slide_3.jpg" alt=""></li></a>
                <a class="slide_bg4" href="#">
                    <li>
                        <img src="@Html.Raw(BaseConfigs.GetUrlCdn)kuaiji/images/slide_4.jpg" alt=""></li></a>
                <a class="slide_bg5" href="#">
                    <li>
                        <img src="@Html.Raw(BaseConfigs.GetUrlCdn)kuaiji/images/slide_5.jpg" alt=""></li></a>
            </ul>
            <a class="s-prev" href="#"></a><a class="s-next" href="#"></a>
            <div class="slider">
                <ul class="clearfix">
                    <li class="current"></li>
                    <li></li>
                    <li></li>
                    <li></li>
                    <li class="last"></li>
                </ul>
            </div>
        </div>
    </div>
    <!--头部轮播 结束-->
    @RenderPage("~/Views/Shared/Nav.cshtml")
</div>
<!--头部 结束-->
@RenderBody()
@section scripts{
    <script src="@{@BaseConfigs.GetUrlCdn}scripts/jquery.placeholder.js"></script>
    <script src="@BaseConfigs.GetStaticPath/scripts/jquery.slider.js"></script>
    <script src="@BaseConfigs.GetStaticPath/scripts/template.js"></script>
    @RenderSection("scripts", required: false)
}
